{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors https://payhip.com https://*.payhip.com https://payhippages.com https://*.payhippages.com 'self';"
        }
      ]
    }
  ]
}
